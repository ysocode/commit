{
    "name": "ysocode/commit",
    "description": "Generate your conventional commit with AI",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "YSOCode\\Commit\\": "src/"
        },
        "files": [
            "src/Foundation/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Yuri Oliveira",
            "email": "yuri.oliveira@ysocode.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "laravel/pint": "^1.21",
        "captainhook/captainhook": "^5.25",
        "captainhook/hook-installer": "^1.0",
        "ramsey/conventional-commits": "^1.6",
        "phpunit/phpunit": "^12.0",
        "rector/rector": "^2.0"
    },
    "bin": [
        "bin/commit"
    ],
    "config": {
        "allow-plugins": {
            "captainhook/hook-installer": true
        }
    }
}
