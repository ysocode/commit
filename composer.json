{
    "name": "ysocode/commit",
    "description": "Generate your conventional commit with AI",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "YSOCode\\Commit\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Yuri Oliveira",
            "email": "yuri.oliveira@ysocode.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.3",
        "symfony/console": "^7.2",
        "illuminate/support": "^12.0",
        "vlucas/phpdotenv": "^5.6",
        "symfony/process": "^7.2",
        "symfony/http-client": "^7.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "laravel/pint": "^1.21",
        "captainhook/captainhook": "^5.25",
        "captainhook/hook-installer": "^1.0",
        "ramsey/conventional-commits": "^1.6"
    },
    "bin": [
        "bin/commit"
    ],
    "config": {
        "allow-plugins": {
            "captainhook/hook-installer": true
        }
    }
}
